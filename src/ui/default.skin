            <![CDATA[
 QMenu {
border: 1px dashed red;color:#4C618F;
     background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0  #EFEFEF,stop: 0.4 #F5F5F5,stop: 0.5 #F6F6F6, stop: 1 #FcFcFc);
 }

 QMenu::item {
     /* sets background of menu item. set this to something non-transparent
         if you want menu color and menu item color to be different */
     background-color: transparent;
 }

 QMenu::item:selected { /* when user selects item using mouse or keyboard */
     background-color: qlineargradient(x1: 0, y1: 0, x2: 0.5, y2: 0.5,
                                                         stop: 0  #D7EEFD,stop: 0.5 #CDE8FC, stop: 1 #BDDDFB);
 }





  QScrollBar:vertical {
      border: 2px solid grey;
      background: #32CC99;
        background-image: url(images/scrollbarbg.bmp);
      width: 15px;
      margin: 22px 0 22px 0;
  }
  QScrollBar::handle:vertical {
      background-image: url(images/btnScrollBarSlider_0_0.bmp);
      min-height: 20px;
    margin:0 0 0 -4px;
  }
  QScrollBar::handle:vertical:hover {
      background-image: url(images/btnScrollBarSlider_2_0.bmp);
      min-height: 20px;
  }
  QScrollBar::handle:vertical:pressed {
      background-image: url(images/btnScrollBarSlider_1_0.bmp);
      min-height: 20px;
  }
  QScrollBar::add-line:vertical {
      border: 2px solid grey;
      background: #32CC99;
      height: 20px;
      subcontrol-position: bottom;
      subcontrol-origin: margin;
    background-image:url(images/scrollbar_bar.bmp);
  }

  QScrollBar::sub-line:vertical {
      border: 2px solid grey;
      background: #32CC99;
      height: 20px;
      subcontrol-position: top;
      subcontrol-origin: margin;
background-image:url(images/scrollbar_bar.bmp);
  }
  QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
      border: 2px solid grey;
      width: 3px;
      height: 3px;
      background: white;
  }

  QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
      background: none;
  }
 





  QScrollBar:horizontal {
      border: 2px solid grey;
      background: #32CC99;
      background-image: url(images/scrollbarbgH.bmp);
      height: 15px;
      margin: 0px 22px 0px 22px;
  }
  QScrollBar::handle:horizontal {
      background-image: url(images/btnScrollBarSliderH_0_0.bmp);

    margin:-4px 0px 0px 0px ;
  }
  QScrollBar::handle:horizontal:hover {
      background-image: url(images/btnScrollBarSliderH_0_2.bmp);


    margin:-4px 0px 0px 0px ;
  }
  QScrollBar::handle:horizontal:pressed {
      background-image: url(images/btnScrollBarSliderH_0_1.bmp);
      
  }
  QScrollBar::add-line:horizontal {
      border: 2px solid grey;
      background: #32CC99;
      width: 20px;
      subcontrol-position: right;
      subcontrol-origin: margin;
     background-image:url(images/scrollbar_bar.bmp);
  }

  QScrollBar::sub-line:horizontal {
      border: 2px solid grey;
      background: #32CC99;
      width: 20px;
      subcontrol-position: left;
      subcontrol-origin: margin;
    background-image:url(images/scrollbar_bar.bmp);
  }
  QScrollBar::left-arrow:horizontal, QScrollBar::right-arrow:horizontal {
      border: 2px solid grey;
      width: 3px;
      height: 3px;
      background: white;
  }

  QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
      background: none;
  }

