        <Tabs>
            <PlayList name="TabPlayList" x="5" y="150" width="289" height="350" hidden="0">
                <Css>
                <![CDATA[
                     #TabPlayList::pane { 
                         border-top: 0px solid #C2C7CB;
                         position: absolute;
                        /* top: -0.5em;*/
                     }

                     #TabPlayList::tab-bar {
                         alignment: center;
                     }

                     QTabWidget::pane { /* The tab widget frame */
                         border-top: 2px solid #C2C7CB;
                         position: absolute;
                         top: -0.5em;
                     }

                     QTabWidget::tab-bar {
                         alignment: center;
                     }

                     /* Style the tab using the tab sub-control. Note that
                         it reads QTabBar _not_ QTabWidget */
                     QTabBar::tab {
                         background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0  #EFEFEF,stop: 0.4 #F5F5F5,stop: 0.5 #F6F6F6, stop: 1 #FcFcFc);
                         border: 2px solid #d4C4C3;
                         border-bottom-color: #d2C7CB; /* same as the pane color */
                         border-bottom-left-radius: 10px;
                         border-top-right-radius: 10px;
                         min-width: 12ex;
                         padding: 3px;
                     }

                     
                ]]>
                </Css>                
            </PlayList>            
        </Tabs>
        
